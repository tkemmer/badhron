fn pending(name: &[u8]) -> () {
    rstack.pending();
}

fn assert_true(body: fn() -> bool) -> () {
    if body() {
        rstack.passed();
    } else {
        rstack.failed();
    }
}

fn assert_false(body: fn() -> bool) -> () {
    if body() {
        rstack.failed();
    } else {
        rstack.passed();
    }
}

fn assert_bool(exp: bool, body: fn() -> bool) -> () {
    if exp == body() {
        rstack.passed();
    } else {
        rstack.failed();
    }
}

